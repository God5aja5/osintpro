{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt",
  "env": {
    "SECRET_KEY": "@secret_key",
    "SEARCH_COST": "2"
  }
}
