{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt",
  "env": {
    "SECRET_KEY": "@secret_key",
    "PROXY": "@proxy",
    "USER_AGENT": "@user_agent",
    "INSTAGRAM_API_KEY": "@instagram_api_key",
    "FIREBASE_API_KEY": "@firebase_api_key",
    "SEARCH_COST": "2"
  }
}